{
  "manifest_version": 2,

  "name": "chromise_test",
  "description": "The tests of the chromise library.",
  "version": "0.1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "tabs"
  ],
  "content_security_policy": "script-src 'unsafe-eval'; object-src 'self'"
}
